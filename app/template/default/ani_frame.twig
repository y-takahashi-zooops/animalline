    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <script src="https://kit.fontawesome.com/330557fe96.js" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/css/base-style.css') }}">

    <link rel="stylesheet" href="{{ asset('assets/css/common.css') }}">
    {% block stylesheet %}{% endblock %}
</head>
<body class="bg-primary" style="background-color:#FFFF">
    <div id="wrapper">
    
        <header>
            <div class="wrap">
            <div class="header-left">
                <div class="header-logo">
                    <a href="{{url('ani_homepage')}}">
                        <img src="{{ asset('assets/img/logo.png') }}" width="610" height="80" alt="Animalline"/>
                    </a>
                </div>
                <!--
                <div class="header-count">
                <div class="header-count__dogs"><span>子犬掲載数</span><strong><i>2,652</i>頭</strong></div>
                <div class="header-count__breeders"><span>ブリーダー数</span><strong><i>2,562</i>人</strong></div>
                -->
                </div>
            </div>
            
            <div class="btn-trigger">
                        <span></span>
                        <span></span>
                        <span></span>
                    </div>
            </div>
        </header>
        <main role="main">
            {% block main %}{% endblock %}
            <div class="pagetop">
                <a href="#wrapper"><i class="bi bi-arrow-bar-up"></i></a>
            </div>
        </main>

        <footer>
            <div class="inner">
            <div class="footer-row">
                <div class="footer-link">
                <a href="{{url('ani_company')}}">会社概要</a>｜<a href="{{url('ani_policy')}}">プライバシーポリシー</a>
                </div>
            </div>
            <div class="footer-logo"><a href="{{url('ani_homepage')}}"><img src="{{ asset('assets/img/logo.png') }}" width="305" height="40" alt="Animalline"/></a></div>
            <div class="copyright"><small>Copyright © 2021 Animalline All Rights Reserved.</small></div>
            </div>
        </footer>
    
        <div id="side-menu">
            <div class="side-menu__count">
                <div class="header-count__dogs"><span>子犬掲載数</span><strong><i>2,652</i>頭</strong></div>
                <div class="header-count__breeders"><span>ブリーダー数</span><strong><i>2,562</i>人</strong></div>
            </div>
            <div class="side-menu__list">
               
            </div>
        </div>
    </div>
</body>
